#########################################
#		Domain Name & 42 login			#
#########################################
	LOGIN=your_42_login


#########################################
#		MySQL/MariaDB Configuration		#
#########################################
	MYSQL_DATABASE=my_mariadb					# Name of DB that stores Wordpress Data
	MYSQL_USER=db_user							# Username Worpress uses to access mariaDB


#########################################
#		WordPress Configuration			#
#########################################
# Wordpress Admin username cannot contain (case-insensitive)
	# 'Admin'
	# 'Administrator'

	WP_PAGE_TITLE=your_page_title
	WP_ADMIN_USER_ENV=your_admin_username
	WP_ADMIN_EMAIL=admin@example.com
	WP_USER=regular_user						# Second Wordpress user
	WP_USER_EMAIL=user@example.com

#########################################
#		SSL Certificate paths			#
#########################################
	SSL_CERT=/etc/nginx/ssl/nginx.crt
	SSL_KEY=/etc/nginx/ssl/nginx.key